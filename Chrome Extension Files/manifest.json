{
  "manifest_version": 3,
  "name": "Privacy Rater",
  "version": "1.1",
  "description": "Rates website privacy policies using AI.",
  "permissions": ["activeTab", "scripting"],
"host_permissions": ["<all_urls>", "https://openrouter.ai/*"]
,
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Check Privacy Rating"
  }
  
}
